{
  "name": "back",
  "version": "0.0.0",
  "private": true,
  "main": "dist/back.js",
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "watch": "rollup -c -w",
    "dev": "concurrently --kill-others-on-fail -p name -n \"BUILD,SERVER\" -c \"yellow.bold,blue.bold\" \"npm run watch\" \"onchange dist/back.js -- npm run start\"",
    "start": "node dist/back.js",
    "serve": "NODE_ENV=production node dist/back.js"
  },
  "dependencies": {
    "@koa/cors": "2",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "lodash.merge": "^4.6.1",
    "mongodb": "^3.1.4"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "babelrc-rollup": "^3.0.0",
    "concurrently": "^3.6.0",
    "onchange": "^4.0.0",
    "rollup": "^0.61.1",
    "rollup-plugin-auto-external": "^1.2.0",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-watch": "^4.3.1"
  }
}
